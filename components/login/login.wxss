.login_warp {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 999;
}

.login_content {
  margin-top: -100rpx;
  width: 600rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 15rpx;
  overflow: hidden;
}

.login_tip {
  text-align: center;
  font-size: 45rpx;
  font-weight: bold;
  line-height: 80rpx;
}

.login_msg {
  font-size: 30rpx;
  line-height: 45rpx;
  text-align: center;
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx;
}

.login_btn {
  width: 100%;
  box-sizing: border-box;
  border-top: 1rpx solid #eee;
  display: flex;
  justify-content: center;
  align-items: center;
}

.login_btn button {
  width: 50%;
  box-sizing: border-box;
  border: none;
  background-color: #fff;
  border-radius: 0;
}

.login_btn button:nth-child(1) {
  border-right: 1px solid #eee;
}
