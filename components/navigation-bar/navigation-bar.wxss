.nav-wrap {
    position: fixed;
    width: 100%;
    top: 0;
    z-index: 800;
}

/* 标题要居中 */

.nav-content {
    position: relative;
}

.nav-title {
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    font-size: 36rpx;
    font-weight: 600;
    line-height: 40px;
    text-align: center;
}

.nav-capsule {
    display: flex;
    align-items: center;
    margin-left: 30rpx;
    width: 120rpx;
    justify-content: space-between;
    height: 100%;
    z-index: 999;
}

.navbar-v-line {
    width: 1px;
    height: 32rpx;
}

.back-pre, .back-home {
    width: 32rpx;
    height: 36rpx;
    margin-top: 4rpx;
    padding: 10rpx;
}

.nav-capsule .back-home {
    width: 36rpx;
    height: 40rpx;
    margin-top: 3rpx;
}

.point-wrap {
    height: 100%;
    width: 100%;
    text-align: center;
    position: relative;
}

.point {
    width: 300rpx;
    height: 64rpx;
    position: absolute;
    bottom: 0;
    right: 102px;
}

.pointAnimation {
    animation: point 2s linear infinite;
}

@keyframes point {
    from {
        transform: translateX(0rpx);
    }

    25% {
        transform: translateX(15rpx);
    }

    50% {
        transform: translateX(0rpx);
    }

    75% {
        transform: translateX(-15rpx);
    }

    to {
        transform: translateX(0rpx);
    }
}

.left_title {
    font-size: 36rpx;
    font-weight: 600;
    padding-left: 30rpx;
}


